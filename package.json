    "@types/node": "^16.18.113",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^26.6.0",
    "babel-plugin-istanbul": "^6.1.1",
    "concurrently": "^6.4.0",
